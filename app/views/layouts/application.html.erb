<html class=" js no-touch svg inlinesvg svgclippaths no-ie8compat" lang="en" style="">

  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="robots" content="noindex">
    <link rel="shortcut icon" type="image/x-icon" href="//cdn.shopify.com/s/files/1/0359/5777/t/6/assets/favicon.png?20742">
    <link rel="canonical" href="http://blog.code-by-kids.com/pages/about">

    <title>codeforkids-staging - About Code For Kids</title>

    <%= render template: 'layouts/meta' %>

    <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
    <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
    <%= csrf_meta_tags %>

    <!-- STYLESHEETS -->
    <link href="//cdn.shopify.com/s/files/1/0359/5777/t/6/assets/normalize.css?20742" rel="stylesheet" type="text/css" media="all">
    <link href="//cdn.shopify.com/s/files/1/0359/5777/t/6/assets/font-awesome.min.css?20742" rel="stylesheet" type="text/css" media="all">
    <link href="//cdn.shopify.com/s/files/1/0359/5777/t/6/assets/foundation.min.css?20742" rel="stylesheet" type="text/css" media="all">
    <link href="//cdn.shopify.com/s/files/1/0359/5777/t/6/assets/global.scss.css?20742" rel="stylesheet" type="text/css" media="all">
    <link href="//cdn.shopify.com/s/files/1/0359/5777/t/6/assets/overrides.css?20742" rel="stylesheet" type="text/css" media="all">
    <link href="//cdn.shopify.com/s/files/1/0359/5777/t/6/assets/slicknav.css?20742" rel="stylesheet" type="text/css" media="all">
    <link href="//cdn.shopify.com/s/files/1/0359/5777/t/6/assets/timeline.css?20742" rel="stylesheet" type="text/css" media="all">

    <!-- SCRIPTS -->
    <script type="text/javascript" async="" src="//cdn.shopify.com/s/javascripts/shopify_stats.js?v=6" style=""></script>
    <script type="application/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>
    <script src="http://maps.gstatic.com/intl/en_us/mapfiles/api-3/16/6/main.js" type="text/javascript"></script>
    <script src="//cdn.shopify.com/s/files/1/0359/5777/t/6/assets/modernizr.js?20742" type="text/javascript"></script>
    <script src="//cdn.shopify.com/s/files/1/0359/5777/t/6/assets/jquery.js?20742" type="text/javascript"></script>
    <!--[if IE]>
      <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
  </head>

  <body>
    <!-- START HEADER -->
    <header class="wrap bg-black menu">
      <div class="row">
        <div class="columns">
          <a href="/" class="left" id="hdr-logo" title="Go to Code For Kids Homepage">
            <img src="//cdn.shopify.com/s/files/1/0359/5777/t/6/assets/logo.png?20742" alt="Go to Code For Kids Homepage">
          </a>
          <%= render template: 'layouts/menu' %>
        </div>
      </div>
    </header>
    <!-- END HEADER -->

    <!-- START CONTENT -->
    <div class="content">

      <!-- START SPLASH HEADER -->
      <section class="page-splash splash-about text-center wrap">
        <div class="row">
          <div class="columns">
            <h1>Code For Kids Wiki</h1>
            <h4 style="color:white">This is a free wiki system for Code For Kids Information</h4>
          </div>
        </div>
      </section>
      <!-- END SPLASH HEADER -->

      <% if session[:user_id] %>
              <%= render template: 'layouts/admin_bar' %>
      <% end %>

      <div class="content">
        <div class="row">
          <div class="columns">
            <%= yield %>
          </div>
        </div>
      </div>

    </div>
     <!-- END CONTENT -->

    <!-- START FOOTER -->
    <%= render template: 'layouts/footer' %>
    <!-- END FOOTER -->

    <script src="//cdn.shopify.com/s/files/1/0359/5777/t/6/assets/jquery.slicknav.min.js?20742" type="text/javascript"></script>
    <script>
      $('#menu').slicknav();
    </script>

  </body>

</html>
